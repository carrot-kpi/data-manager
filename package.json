{
    "type": "module",
    "private": true,
    "name": "@carrot-kpi/data-manager",
    "version": "0.1.0",
    "author": "Federico Luzzi <federico.luzzi@carrot-labs.xyz>",
    "license": "GPL-3.0-or-later",
    "description": "A service to facilitate data management in Carrot.",
    "scripts": {
        "prepare": "node ./scripts/prepare.js",
        "commitlint": "commitlint -e",
        "lint": "eslint --ext .js ./ && prettier --check './**/*.{json,md}'",
        "format": "eslint --fix --ext .js ./ && prettier --write './**/*.{json,md}'",
        "generate-jwt": "tsx scripts/generate-jwt.ts",
        "upload-folder": "tsx scripts/upload-folder.ts",
        "build": "esbuild ./src/index.ts --platform=node --format=esm --define:process.env.NODE_ENV=\\\"production\\\" --bundle --packages=external --minify --outdir=./out --out-extension:.js=.mjs",
        "dev": "nodemon --exec tsx src/index.ts"
    },
    "devDependencies": {
        "@commitlint/cli": "^18.4.4",
        "@commitlint/config-conventional": "^18.4.4",
        "@smithy/types": "^2.9.1",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/pg": "^8.10.9",
        "dotenv": "^16.3.1",
        "esbuild": "^0.19.11",
        "eslint": "^8.56.0",
        "eslint-config-custom": "*",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "husky": "^8.0.3",
        "nodemon": "^3.0.3",
        "pino": "^8.17.2",
        "prettier": "^3.2.4",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.499.0",
        "@aws-sdk/lib-storage": "^3.499.0",
        "@hapi/boom": "^10.0.1",
        "@hapi/hapi": "^21.3.2",
        "@hapi/inert": "^7.1.0",
        "@hapi/vision": "^7.0.3",
        "@ipld/car": "^5.2.6",
        "@ucanto/core": "^9.0.1",
        "@ucanto/principal": "^9.0.0",
        "@web3-storage/upload-client": "^13.0.0",
        "@web3-storage/w3up-client": "^12.0.0",
        "hapi-pino": "^12.1.0",
        "hapi-swagger": "^17.2.0",
        "joi": "^17.11.1",
        "jsonwebtoken": "^9.0.2",
        "multiformats": "^13.0.1",
        "pg": "^8.11.3",
        "viem": "^2.2.0"
    }
}
