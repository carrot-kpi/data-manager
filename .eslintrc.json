{
    "root": true,
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": "latest"
    },
    "env": {
        "node": true
    },
    "extends": ["prettier", "eslint:recommended"],
    "plugins": ["prettier"],
    "ignorePatterns": ["node_modules/", "out/"],
    "rules": {
        "prettier/prettier": "error"
    }
}
